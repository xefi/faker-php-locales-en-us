{
  "name": "xefi/faker-php-locales-en-us",
  "type": "library",
  "description": "Faker extension to include en_US locale",
  "keywords": [
    "faker",
    "data",
    "en"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Gautier Deleglise"
    }
  ],
  "require": {
    "php": "^8.3",
    "psr/container": "^2.0",
    "xefi/faker-php": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^11"
  },
  "autoload": {
    "psr-4": {
      "Xefi\\Faker\\EnUs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xefi\\Faker\\EnUs\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "faker": {
      "providers": [
        "Xefi\\Faker\\EnUs\\FakerEnUsServiceProvider"
      ]
    }
  }
}
